package com.baemin.dao;

import java.util.List;
import java.util.Map;

import com.baemin.dto.Order;
import com.baemin.dto.OrderList;
import com.baemin.login.LoginDetails;

public interface OrderDAO {

	// 회원 주문하기
	void memberOrder(Map<String, Object> map);

	// 비회원 주문하기
	void nonMemberOrder(Map<String, Object> map);

	// 회원 주문목록
	List<OrderList> memberOrderList(long userId);

	// 비회원 주문목록
	List<OrderList> memberOrderList(String guestId);

}
