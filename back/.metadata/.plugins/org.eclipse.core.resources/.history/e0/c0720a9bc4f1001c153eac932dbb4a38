	<insert id="order">
		<if test="user != null">
			insert into order_user (
				order_num
		    ,user_id
		    ,store_id
		    ,delevery_address
		    ,pay_method 
		    ,used_point 
		    ,total_price
		    ,request 
		    ,imp_uid 
			) values (
				#{order.orderNum}
				,#{user.userId}
				,#{order.address}
				,#{order.payMethod}
				,#{order.usedPoint}
				,#{order.totalPrice}
				,#{order.request}
				,#{order.storeId}
				,''			
			)
			
			
		
		</if>
	
	
	</insert>


	
	
	
</mapper>
