import com.baemin.dto.User;

public interface OrderService {

	ResponseEntity<?> order(Order order, User userInfo);
	
	List<OrderDetail> getMemberOrderList(long userId);

	List<OrderDetail> getNonMemberOrderList(String guestId);
}
