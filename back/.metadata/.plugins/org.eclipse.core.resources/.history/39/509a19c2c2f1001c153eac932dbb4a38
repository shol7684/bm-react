package com.baemin.dto;

import lombok.Data;

@Data
public class Order {
	
	private Cart[] cartList;
	private int usedPoint;
	private int totalPrice;
	
	private String payMethod;
	private String request;
	private String address;
	
	// 로그인 중일때
	private User user;
	// 로그인 중 아닐때
	private String guestId;
}

